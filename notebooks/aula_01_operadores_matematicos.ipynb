        "colab": {}
      },
      "source": [
        "# E se precisarmos colocar um número \"no meio\" do texto?\n",
        "nome = \"Edson Melo tem\"\n",
        "idade = 30\n",
        "complemento = \"anos\"\n",
        "\n",
